<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parallel Testing" parallel="classes" thread-count="5">
	<parameter name="browser" value="Chrome"></parameter>

	<test name="Test1" thread-count="3">
		<classes>


			<class name="testscripts.LoginTest">
				<methods>
					<include
						name="verifyWhetherUserIsAbleToLoginWithValidUsernameAndInvalidPassword">
					</include>
					<include
						name="verifyWhetherUserIsAbleToLoginWithValidCredentials"></include>
				</methods>
			</class>
			<class name="testscripts.HomeTest">
				<methods>
					<include name="verifyWhetherUserIsAbleToLogoutSuccessfully"></include>
				</methods>
			</class>


		</classes>
	</test> <!--
	Test -->
	

</suite> <!--
Suite -->
